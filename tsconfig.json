{
  "compilerOptions":{ 
    "target":"es6",
    "moduleResolution":"node",
    "module":"commonjs",
    "noLib":false,
    "emitDecoratorMetadata":true,
    "experimentalDecorators":true,
    "sourceMap":true,
    "pretty":true,
    "allowUnusedLabels":true,
    "noImplicitAny":true,
    "noImplicitReturns":false,
    "noImplicitUseStrict":false,
    "outDir":"dist/",
    "baseUrl":"src/",
    "listFiles":false,
    "noEmitHelpers":true,
    "resolveJsonModule": true
  },
  "include":[ 
    "**/*.ts"
  ],
  "exclude":[ 
    "node_modules",
    "src/deploy-commands.ts"
  ],
}
