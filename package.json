{
  "name": "coding-factory-discord-bot",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "lint": "eslint src/*.ts --fix",
    "dev": "tsc-watch -p ./tsconfig.json --onSuccess \"concurrently 'node dist/index.js' 'node dist/server/server.js'\"",
    "build": "npm run fix_discord_deps && tsc",
    "fix_discord_deps": "cd node_modules/@discordjs/rest && npm i --save-dev @types/node-fetch && cd ../../../",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@discordjs/builders": "^0.5.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "googleapis": "^85.0.0",
    "typescript": "^4.4.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "tsc-watch": "^4.5.0"
  }
}
